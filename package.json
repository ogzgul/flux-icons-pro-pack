{
  "name": "flux-icons-pro-pack",
  "version": "1.0.1",
  "private": false,
  "description": "1000+ Modern, animated and customizable SVG icons for Vue 3 & Nuxt.",
  "author": "Oguz <oguz.gul216@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist-lib"
  ],
  "main": "./dist-lib/flux-icons.umd.js",
